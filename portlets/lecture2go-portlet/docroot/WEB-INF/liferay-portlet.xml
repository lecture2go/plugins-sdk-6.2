   	 	    <scheduler-description>
      		This scheduler is used to collect Statistical Data from Database
  		 	</scheduler-description> 
  			<scheduler-event-listener-class>de.uhh.l2g.plugins.util.StatisticsScheduler</scheduler-event-listener-class>  
